{
  "manifest_version": 3,
  "name": "Project Tab Manager",
  "description": "Manage your projects directly from Chrome.",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage", "tabs", "activeTab", 
    "webNavigation", "contextMenus", "notifications"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {}
  },
  "icons": {}
}